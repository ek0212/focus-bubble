{
    "manifest_version": 3,
    "name": "Focus Bubble",
    "version": "1.0.0",
    "description": "Stay focused on work and study by gently blocking distractions. Customize your apps, track focus sessions, and build better habits.",
    "permissions": ["tabs", "storage", "scripting", "alarms"],
    "host_permissions": ["<all_urls>"],
    "action": {
      "default_popup": "popup/popup.html",
      "default_icon": {
        "128": "icons/icon128.png"
      }
    },
    "background": {
      "service_worker": "background.js"
    },
    "icons": {
      "128": "icons/icon128.png"
    }
}